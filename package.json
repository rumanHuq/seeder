{
  "author": "You <you@example.com>",
  "name": "rust-webpack-template",
  "version": "0.1.0",
  "scripts": {
    "build": "rm -rf pkg dist && NODE_ENV=production webpack --config ./build-configs/webpack.config.js",
    "start": "rm -rf pkg dist && NODE_ENV=development webpack-dev-server --config ./build-configs/webpack.config.js",
    "test": "cargo test && wasm-pack test --headless",
    "tailwind-build": "postcss build-configs/tailwind.css -o dist/css/tailwind.css"
  },
  "devDependencies": {
    "@wasm-tool/wasm-pack-plugin": "^1.0.1",
    "autoprefixer": "9.7.2",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "find": "^0.3.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-cli": "6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-typed-css-classes": "^0.1.4",
    "style-loader": "^1.0.0",
    "tailwindcss": "^1.1.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
